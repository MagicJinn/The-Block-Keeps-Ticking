{
    "schemaVersion": 1,
    "id": "the-block-keeps-ticking",
    "version": "${version}",
    "name": "The Block Keeps Ticking",
    "description": "A mod to simulate progress in unloaded chunks.",
    "authors": ["MagicJinn"],
    "contact": {
        "homepage": "https://github.com/MagicJinn/Beacon-Aura",
        "issues": "https://github.com/MagicJinn/Beacon-Aura/issues"
    },
    "license": "MIT",
    "icon": "assets/the-block-keeps-ticking/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": ["magicjinn.theblockkeepsticking.TheBlockKeepsTicking"],
        "client": ["magicjinn.theblockkeepsticking.TheBlockKeepsTickingClient"]
    },
    "mixins": [
        "the-block-keeps-ticking.mixins.json",
        {
            "config": "the-block-keeps-ticking.client.mixins.json",
            "environment": "client"
        }
    ],
    "accessWidener": "the-block-keeps-ticking.accesswidener",
    "depends": {
        "fabricloader": "*",
        "minecraft": ">=1.21.0",
        "java": ">=17",
        "fabric-api": "*"
    },
    "suggests": {
        "modmenu": "*",
        "yet-another-config-lib": "*"
    },
    "custom": {
        "modmenu": {
            "links": {
                "modmenu.discord": "https://discord.gg/bQvtauxXWp",
                "modmenu.source": "https://github.com/MagicJinn/The-Block-Keeps-Ticking"
            }
        }
    }
}